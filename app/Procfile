streamupstream: env TARGET=streamupstream docker-gen -watch -notify "nginx -s reload" /app/nginx.tmpl /etc/nginx/stream.conf.d/10-upstream.conf
httpupstream: env TARGET=upstream docker-gen -watch -notify "nginx -s reload" /app/nginx.tmpl /etc/nginx/conf.d/10-upstream.conf
httpserver: env TARGET=server docker-gen -watch -notify "nginx -s reload" /app/nginx.tmpl /etc/nginx/conf.d/default.conf
nginx: nginx -g "daemon off;"
